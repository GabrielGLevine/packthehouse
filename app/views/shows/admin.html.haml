                    %a Add a member
              %li
                %a Invitations
              %li
                %a Cloud Storage Environment Settings
              %li
                %a Authentication
            %p.menu-label
              Transactions
            %ul.menu-list
              %li
                %a Payments
              %li
                %a Transfers
              %li
                %a Balance
        .column.is-9
